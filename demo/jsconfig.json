{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",       
    "moduleResolution": "Node16",
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules", "dist"]
}